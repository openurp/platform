<?xml version="1.0"?>
<profiles>
  <profile name="openurp-ds" pattern="org.openurp.*.ds">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/" suffix=""  style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
      <interceptor name="web.Interceptor.appsecurity"/>
    </interceptors>
  </profile>
</profiles>
