<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.security.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">
  <class name="FieldBean" entity-name="org.beangle.security.blueprint.Field">
    <id name="id" type="integer">
      <generator class="auto_increment" />
    </id>
    <property name="name" length="30" not-null="true" />
    <property name="title" length="40" not-null="true" />
    <property name="source" length="500" />
    <property name="multiple" not-null="true" />
    <property name="required" not-null="true" />
    <property name="typeName" not-null="true" />
    <property name="keyName" length="20"/>
    <property name="properties" length="100"/>
  </class>
</hibernate-mapping>