<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
  xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.app.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">

  <class name="DataSourceBean">
    <cache usage="read-write" />
    <id name="id" unsaved-value="null" type="integer">
      <generator class="date" />
    </id>
    <many-to-one name="app" not-null="true" />
    <many-to-one name="db" not-null="true" />
    <property name="username" not-null="true" />
    <property name="password" not-null="true" />
    <property name="maxActive" not-null="true"/>
    <property name="name" not-null="true"/>
    <property name="remark" />
  </class>

</hibernate-mapping>