<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.app.func.model" default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">

  <class name="AppFuncPermissionBean" abstract="true" entity-name="org.openurp.app.func.AppFuncPermission">
    <id name="id" type="integer">
      <generator class="auto_increment" />
    </id>
    <many-to-one name="app" not-null="true"/>
    <many-to-one name="resource" not-null="true" />
    <property name="actions"/>
    <property name="restrictions"/>
  </class>

</hibernate-mapping>