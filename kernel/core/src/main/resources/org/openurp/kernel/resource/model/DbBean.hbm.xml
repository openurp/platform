<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
  xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.kernel.resource.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">

  <class name="DbBean">
    <cache usage="read-write" />
    <id name="id" unsaved-value="null" type="integer">
      <generator class="auto_increment" />
    </id>
    <property name="name" unique="true" not-null="true" />
    <property name="url" not-null="true" />
    <property name="driverClassName" not-null="true" />
    <property name="remark" />
  </class>

</hibernate-mapping>