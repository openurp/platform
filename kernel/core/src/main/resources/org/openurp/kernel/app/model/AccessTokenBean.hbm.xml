<?xml version="1.0" encoding="UTF-8"?>
<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping"
  xsi:schemaLocation="http://www.hibernate.org/xsd/hibernate-mapping classpath://org/hibernate/hibernate-mapping-4.0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" package="org.openurp.kernel.app.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">

  <class name="AccessTokenBean">
    <cache usage="read-write" />
    <id name="id" unsaved-value="null" type="integer">
      <generator class="auto_increment" />
    </id>
    <many-to-one name="app" unique="true"  not-null="true" />
    <property name="token"  unique="true" length="100" not-null="true" />
    <property name="expiredAt" not-null="true" />
  </class>
</hibernate-mapping>