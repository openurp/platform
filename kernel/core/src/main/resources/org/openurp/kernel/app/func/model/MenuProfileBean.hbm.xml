<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.openurp.kernel.app.func.model"
  default-access="org.beangle.data.jpa.hibernate.PropertyAccessor">

  <class name="MenuProfileBean" entity-name="org.openurp.kernel.app.func.MenuProfile">
    <id name="id" type="integer">
      <generator class="auto_increment" />
    </id>
    <many-to-one name="app" not-null="true" />
    <property name="name" not-null="true" length="100" />
    <set name="menus" collection-type="seq">
      <key column="profile_id" />
      <one-to-many  class="org.openurp.kernel.app.func.Menu" />
    </set>
  </class>

</hibernate-mapping>