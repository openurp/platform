<?xml version="1.0"?>
<profiles>
  <!-- FIXME support parent -->
  <profile name="openurp-securityws" pattern="org.openurp.security.ws">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/" suffix=""  style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
    </interceptors>
  </profile>
  <profile name="openurp-appws" pattern="org.openurp.app.ws">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/" suffix=""  style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
    </interceptors>
  </profile>
</profiles>
