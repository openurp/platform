<?xml version="1.0"?>
<mvc>
  <profile name="openurp-platform-ws" package="org.openurp.platform.ws">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/" suffix="" style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
    </interceptors>
  </profile>
  
  <profile name="openurp-platform-ws-security-func" package="org.openurp.platform.ws.security.func">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/security/func/{app}/" suffix="" style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
    </interceptors>
  </profile>

  <profile name="openurp-platform-ws-security-data" package="org.openurp.platform.ws.security.data">
   <action suffix="WS" defaultMethod="index"/>
   <url path="/security/data/" suffix="" style="plur-seo" />
   <interceptors>
      <interceptor name="web.Interceptor.hibernate"/>
      <interceptor name="web.Interceptor.cors"/>
    </interceptors>
  </profile>
  
</mvc>
