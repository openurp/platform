<?xml version="1.0" encoding="UTF-8"?>
<ehcache:config
    xmlns:ehcache="http://www.ehcache.org/v3"
    xmlns:jcache="http://www.ehcache.org/v3/jsr107">

    <ehcache:cache-template name="session_data.Template">
      <ehcache:expiry>
        <ehcache:ttl unit="minutes">30</ehcache:ttl>
      </ehcache:expiry>
      <ehcache:heap size="5000" unit="entries"/>
    </ehcache:cache-template>
    <ehcache:cache-template name="session_status.Template">
      <ehcache:expiry>
        <ehcache:tti unit="minutes">30</ehcache:tti>
      </ehcache:expiry>
      <ehcache:heap size="5000" unit="entries"/>
    </ehcache:cache-template>
</ehcache:config>
