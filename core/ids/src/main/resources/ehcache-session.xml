<?xml version="1.0" encoding="UTF-8"?>
<ehcache:config
    xmlns:ehcache="http://www.ehcache.org/v3"
    xmlns:jcache="http://www.ehcache.org/v3/jsr107">

    <ehcache:cache-template name="session_data.Template">
      <ehcache:key-type>java.lang.String</ehcache:key-type>
      <ehcache:value-type>org.beangle.security.session.Session$Data</ehcache:value-type>
      <ehcache:expiry>
        <ehcache:tti unit="minutes">30</ehcache:tti>
      </ehcache:expiry>
      <ehcache:heap unit="entries">10000</ehcache:heap>
    </ehcache:cache-template>

    <ehcache:cache alias="session_status">
      <ehcache:key-type>java.lang.String</ehcache:key-type>
      <ehcache:value-type>org.beangle.security.session.Session$Status</ehcache:value-type>
      <ehcache:expiry>
        <ehcache:tti unit="minutes">30</ehcache:tti>
      </ehcache:expiry>
      <ehcache:heap unit="entries">10000</ehcache:heap>
    </ehcache:cache>

    <ehcache:cache alias="session_service">
      <ehcache:key-type>java.lang.String</ehcache:key-type>
      <ehcache:value-type>java.util.List</ehcache:value-type>
      <ehcache:expiry>
        <ehcache:tti unit="minutes">30</ehcache:tti>
      </ehcache:expiry>
      <ehcache:heap unit="entries">10000</ehcache:heap>
    </ehcache:cache>
</ehcache:config>
